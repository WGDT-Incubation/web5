{
         "compilerOptions": {
           "target": "ES6",
           "module": "commonjs",
           "rootDir": "./",
           "outDir": "./dist",
           "strict": true,
           "esModuleInterop": true,
           "skipLibCheck": true,
           "resolveJsonModule": true,
           "allowImportingTsExtensions": true,
           "noEmit": true
         },
         "include": ["src/**/*.ts"],
         "exclude": ["node_modules", "dist"]
}
       